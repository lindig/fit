#! /usr/bin/env jq -rf

def header:
  ["distance"
  ,"speed"
  ,"cadence"
  ]
;

def cond:
  .msg == "record"
  and has("distance")
  and (has("speed") or has("enhanced_speed"))
  and has("cadence")
;

def fields:
  [.distance
  ,.speed
  ,.enhanced_speed
  ,.cadence
  ]
;

(.[]|select(cond)|fields)|@csv


