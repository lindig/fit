(executable
 (name main)
 (public_name fit)
 (modules build main)
 (libraries yojson cmdliner fit))

(executable
 (name srt)
 (public_name fit2srt)
 (modules srt build)
 (libraries yojson cmdliner fit ptime unix))

(rule
 (target build.ml)
 (deps (universe) build.sh)
 (action
  (with-stdout-to
   %{target}
   (bash "./build.sh"))))
